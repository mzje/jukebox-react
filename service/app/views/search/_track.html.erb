<tr class="<%= rated_class(track,current_user) %>" data-file="<%= track.file.try(:force_encoding, "UTF-8") %>">
  <td class="track_title">
    <%= track.title.try(:force_encoding, "UTF-8") %>
    <b>(<%= track.duration %>)</b>
    <a href="/search?artist=<%= track.artist.try(:force_encoding, "UTF-8") %>&amp;similar=true&amp;title=<%= track.title.try(:force_encoding, "UTF-8") %>" class="search_helper info_link" title="Similar tracks"><%= icon 'question-circle' %></a>
  </td>
  <td class="track_artist">
    <a href="/search?query=<%= track.artist.try(:force_encoding, "UTF-8") %>&amp;type=artist"><%= track.artist.try(:force_encoding, "UTF-8") %></a>
  </td>
  <td class="track_album">
    <a href="/search?query=<%= track.album.try(:force_encoding, "UTF-8") %>&amp;type=album"><%= track.album.try(:force_encoding, "UTF-8") %></a>
  </td>
  <td class="track_owner">
    <span class="<%= track.source %>" title="<%= track.source.humanize %>"></span>
  </td>
  <td class="track_rating"><%= track_rating_tags track %></td>
  <td class="track_action"><%= add_remove_button(track) %></td>
</tr>